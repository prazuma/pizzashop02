<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Myage</servlet-name>
		<servlet-class>jp.tuyano.MyageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Myage</servlet-name>
		<url-pattern>/myage</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Login.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Myage2</servlet-name>
		<servlet-class>jp.tuyano.MyageSevlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Myage2</servlet-name>
		<url-pattern>/myage2</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>add_linkdata</servlet-name>
		<servlet-class>jp.tuyano.AddLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add_linkdata</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>edit_linkdata</servlet-name>
		<servlet-class>jp.tuyano.EditLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>edit_linkdata</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>del_linkdata</servlet-name>
		<servlet-class>jp.tuyano.DelLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>del_linkdata</servlet-name>
		<url-pattern>/del</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>shoppeple_link</servlet-name>
		<servlet-class>jp.tuyano.Shoppeple</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>shoppeple_link</servlet-name>
		<url-pattern>/shop</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>add_2</servlet-name>
		<servlet-class>jp.tuyano.AddLinkDataServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add_2</servlet-name>
		<url-pattern>/add2</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>edit_linkdata2</servlet-name>
		<servlet-class>jp.tuyano.EditLinkDataServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>edit_linkdata2</servlet-name>
		<url-pattern>/edit2</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>del_linkdata2</servlet-name>
		<servlet-class>jp.tuyano.DelLinkDataServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>del_linkdata2</servlet-name>
		<url-pattern>/del2</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>card</servlet-name>
		<servlet-class>jp.tuyano.CardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>card</servlet-name>
		<url-pattern>/card</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>servlet</servlet-name>
		<servlet-class>jp.tuyano.MyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>servlet</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>logincheck</filter-name>
		<filter-class>jp.tuyano.LoginCheckFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>logincheck</filter-name>
		<url-pattern>/index.html</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>jp.tuyano.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
</web-app>